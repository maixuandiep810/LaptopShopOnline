@model IEnumerable<LaptopShopOnline.Model.Entities.Contact>
@{
    int i = 0;
}

<div class="footer" id="Contact">
    <div class="container">
        <div class="footer-top">
            @if (ViewBag.Count > 0)
            {
                foreach (var item in Model)
                {
                    <div class="col-md-4 footer-left">
                        <h3>@item.Title</h3>
                        <ul>
                            <li><b>@item.ObjectName</b></li>
                            <li>Địa chỉ: <a href="#">@item.Address</a></li>
                            <li>Sđt: <a href="#">@item.Mobile</a></li>
                            <li>Website: <a href="#">@item.Website</a></li>
                            <li>Email: <a href="#">@item.Email</a></li>
                        </ul>
                    </div>
                }
                <div class="col-md-4 footer-left">
                    <h3>Địa chỉ</h3>
                    <ul>
                        <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d15338.160619329805!2d108.220819!3d16.037436!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x982c8e9942da592b!2zS0lNIEFOSCBDT01QVVRFUiBMQVBUT1AgQ8WoLSAzOCBwaGFuIMSQxINuZyBMxrB1LCDEkMOgIE7hurVuZw!5e0!3m2!1svi!2s!4v1617783855621!5m2!1svi!2s" width="340" height="200" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
                    </ul>
                </div>
            }
            <div class="clearfix"> </div>
        </div>
    </div>
</div>